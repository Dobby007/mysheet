a 
    color #fff
    text-decoration: none;

a:hover {
    
    border-bottom 1px dotted #fff
}

a.style-2
    color: #428BCA;
a.style-2:hover
    color #428BFF;

h1 
    font-size 220%
h2
    font-size 180%
h3
    font-size 160%

p
    margin 7pt 0

b
    font-weight bold

.clear
    clear both

h1,h2
    margin-top: 5pt
    margin-bottom 3pt
    line-height 1em
pre
    box-shadow none
    -moz-box-shadow none
    -webkit-box-shadow none
    border-radius 0
    color #000
    margin 10px 0
    padding 10px
    background-color #eee
    border 1px dashed black
    color black
    line-height 110%
    font-size 10pt
    color #1B3341


pre code
    border 0
    color #1B3341

ul
    list-style circle

ol
    list-style decimal

ul.simple
    list-style none
    margin 0
    padding 0

.edit-box
    .memo
        padding: 5px
        background-color #fff
        padding 5px
        border 1px solid #666
        textarea
            background none
            border 0
            padding 0
            width 100% !important
            margin 0
.width50
    width 50%

.tbl-two-sections
    .col1
        padding-right 10px
        width 50%
    .col2
        padding-left 10px
        width 50%